{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/no-multi-comp": 0,
    "react/jsx-filename-extension": 0,
    "no-unused-vars": [
      "error",
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "react/sort-comp": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/no-array-index-key": 1
  }
}
